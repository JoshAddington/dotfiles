	"name": "vscode-dash",
	"displayName": "Dash",
	"description": "Dash integration in Visual Studio Code",
	"version": "1.1.2",
	"publisher": "deerawan",
	"icon": "images/dash.png",
	"galleryBanner": {
		"color": "#7171AD",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^0.10.1"
	},
	"bugs": {
		"url": "https://github.com/deerawan/vscode-dash/issues",
		"email": "deerawan@gmail.com"
	},
	"homepage": "https://github.com/deerawan/vscode-dash",
	"repository": {
		"type": "git",
		"url": "https://github.com/deerawan/vscode-dash"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.dash.specific",
		"onCommand:extension.dash.all"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.dash.specific",
				"title": "Search in Dash for current syntax"
			},
			{
				"command": "extension.dash.all",
				"title": "Search in Dash for all documentation"
			}
		],
		"keybindings": [
			{
				"command": "extension.dash.specific",
				"key": "ctrl+h",
				"mac": "ctrl+h",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.dash.all",
				"key": "ctrl+alt+h",
				"mac": "ctrl+alt+h",
				"when": "editorTextFocus"
			}
		],
